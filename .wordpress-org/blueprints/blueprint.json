{
    "$schema": "https://playground.wordpress.net/blueprint-schema.json",
    "landingPage": "/?p=1",
    "preferredVersions": {
        "php": "latest",
        "wp": "latest"
    },
    "phpExtensionBundles": [
        "kitchen-sink"
    ],
    "login": true,
    "steps": [{
            "step": "setSiteOptions",
            "options": {
                "blogname": "AuRise Demonstration",
                "blogdescription": "Demonstrating the plugin called Socialized"
            }
        },
        {
            "step": "installPlugin",
            "pluginZipFile": {
                "resource": "wordpress.org/plugins",
                "slug": "socialized"
            },
            "options": {
                "activate": true
            }
        }
    ]
}